<Window x:Class="FPECORE.CustomIPropertyDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Custom IProperty" VerticalAlignment="Center" HorizontalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" UseLayoutRounding="False" WindowStartupLocation="CenterScreen" SizeToContent="WidthAndHeight" Width="300" MinWidth="300" ResizeMode="NoResize">
    <Grid>
        <StackPanel Margin="10">
            <TextBlock Text="Введите имя Custom IProperty:" Margin="0,0,0,10"/>
            <TextBox x:Name="propertyNameTextBox" Height="30" VerticalContentAlignment="Center"/>
            <Button Content="OK" IsDefault="True" Click="OkButton_Click" Height="30" Margin="0,10,0,0"/>
        </StackPanel>
    </Grid>
</Window>
